# Application settings
PROJECT_NAME=Prysm Backend
API_V1_STR=/api/v1
APP_NAME=Event Recommendation System
APP_VERSION=1.0.0
ENVIRONMENT=development
HOST=0.0.0.0
PORT=8000
DEBUG=True
RELOAD=True

# Database settings
DATABASE_URL=postgresql://localhost/prysm
POSTGRESQL_CONNECTION_URI=postgresql://user:password@localhost:5432/prysm

# Redis settings
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
